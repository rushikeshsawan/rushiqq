



  <!-- Modal Structure -->
  <div id="gettransactiondetails" class="modal">
    <div class="modal-content center-align ">
      <h4> Transaction Details</h4>
      <form id="idForm" action="/addnewproduct" method="post">
        <div class="mb-3">
          <label for="exampleInputEmail1" class="form-label">BlockHash</label>
          <input type="text" id="blockHash" readonly name="blockHash" placeholder="Blockhash" class="form-control center-align" id="exampleInputEmail1" aria-describedby="emailHelp">
          
        <div class="mb-3">
          <label for="exampleInputPassword1" class="form-label">Block Number</label>
          <input type="text" id="blockNo" readonly placeholder="Blocknumber" name="blocknumber" class="form-control center-align" id="exampleInputPassword1">
        </div>
        <div class="mb-3">
            <label for="exampleInputPassword1" class="form-label">Cumulative Gas Used</label>
            <input type="text" id="cgu" readonly placeholder="Enter Mobile Number" name="mobile" class="form-control center-align" id="exampleInputPassword1">
          </div>
          <div class="mb-3">
            <label for="exampleInputPassword1" class="form-label">Transaction date</label>
            <input type="text" id="td" readonly placeholder="Transaction date" name="email" class="form-control center-align" id="exampleInputPassword1">
          </div>
          <div class="mb-3">
            <label for="exampleInputPassword1" class="form-label">From Address</label>
            <input type="text" id="fa" readonly placeholder="From Address" name="from" class="form-control center-align" id="exampleInputPassword1">
          </div>
          <div class="mb-3">
            <label for="exampleInputPassword1" class="form-label">To Address</label>
            <input type="text" id="ta" readonly placeholder="To Address" name="to" class="form-control center-align" id="exampleInputPassword1">
          </div>
          <div class="mb-3">
            <label for="exampleInputPassword1" class="form-label">Gas Used</label>
            <input type="text" id="gu" readonly placeholder="Gas Used" name="GasUsed" class="form-control center-align" id="exampleInputPassword1">
          </div>
          <div class="mb-3">
              <label for="exampleInputPassword1" class="form-label">Transaction Index</label>
              <input type="text" id="ti" readonly placeholder="Transaction Index" name="TransactionIndex" class="form-control center-align" id="exampleInputPassword1">
            </div>
            <div class="mb-3">
              <label for="exampleInputPassword1" class="form-label">Transaction Hash</label>
              <input type="text" id="th" readonly placeholder="Transaction Hash" name="TransactionHash" class="form-control center-align" id="exampleInputPassword1">
            </div>
        
       </div>
        
            <!-- <button href="#!" class=" modal-close waves-effect waves-green btn-flat">Submit</a> -->
        </div>
        
      </form>
    </div>
  
  </div>