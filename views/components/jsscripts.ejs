<script>
  const params = window.location.href;
  if (params.search('welcome') > 0) {
    // enlargediv.removeClass('medium');
    // enlargediv.removeClass('large');
    $("#welcome").addClass("active");
    
  } else if (params.search('listsold') > 0) {
    
    $("#listsold").addClass("active");
    document.title = "List of Sold Products";
  }else if (params.search('listunsold') > 0) {
    
    $("#listunsold").addClass("active");
    document.title = "List of Unsold Products";
    
  }
  else if (params.search('getproductbyqr') > 0) {
    
    $("#getproducttbyqr").addClass("active");
    document.title = "Scan QR Code";
    
  }
  else if (params.search('profile') > 0) {
    
    $("#profile").addClass("active");
    document.title = "Profile Details";

  }
</script>
